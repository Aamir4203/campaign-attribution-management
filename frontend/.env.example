# CAM Frontend Environment Variables
# Copy this file to .env for development or .env.production for production

# API Configuration
VITE_API_BASE_URL=http://localhost:5000

# Feature Flags
VITE_ENABLE_FILE_UPLOAD=true
VITE_ENABLE_HYBRID_MODE=true
VITE_ENABLE_ADVANCED_VALIDATION=true

# Upload Configuration
VITE_MAX_FILE_SIZE_MB=50
VITE_ALLOWED_FILE_TYPES=csv,xlsx,xls

# Debug Mode
VITE_DEBUG_MODE=true
